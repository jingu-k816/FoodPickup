<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Food pickup</title>

    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
    <link rel="stylesheet" href="/styles/nav.css" type="text/css" />
    <link rel="stylesheet" href="/styles/restaurant.css" type="text/css" />

    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>
  </head>

  <body>
    <%- include('partials/_navbar') %>
    <!-- hide any links on nav bar -->
    <div class="mainRestaurant">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th scope="col">Order#</th>
            <th scope="col">Name</th>
            <th class="orderStat" scope="col">Order status</th>
          </tr>
        </thead>

        <% for(const order of orders) { %>
        <tbody>
          <tr>
              <th scope="row"><%= order.id %></th>
              <td><%= order.name %></td>
              <td><button type="submit">Accept</button></td>
          </tbody>
        <% } %>
      </table>
    </div>
  </body>
</html>
